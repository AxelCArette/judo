		{% endblock %}
	{% endblock %}
</html></head><body>

<nav class="navbar navbar-expand-lg navbar-light navbar-custom">
	<div class="container-fluid">
	<a class="navbar-brand navbar-custom" href="{{ path('app_accueil') }}">
				<img src="{{ asset('asset/img/logojudo.png') }}" width="75" height="75" class="d-inline-block align-top" alt="">
				</a>
		<div class="row w-100">
		
		
			<div class="col-1"></div> <!-- Colonne vide à gauche -->
			
			<div class="col-10">
				
				<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="collapse navbar-collapse navbar-custom" id="navbarNav">
					<ul class="navbar-nav mx-auto">
						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle navbar-custom main-link" href="#" id="navbarDropdownClub" role="button" data-bs-toggle="dropdown" aria-expanded="false">
								<b class="navbar-custom">Le club</b>
							</a>
							<ul class="dropdown-menu" aria-labelledby="navbarDropdownClub">
								<li><a class="dropdown-item" href="{{ path('app_horaire_et_tarif') }}">A propos</a></li>
								<li><a class="dropdown-item" href="{{ path('app_histoire_duclub') }}">Histoire du club</a></li>
								<li><a class="dropdown-item" href="{{ path('app_notre_bureau') }}">Notre bureau</a></li>
								<li><a class="dropdown-item" href="{{ path('app_sponsors') }}">Nos sponsors</a></li>
							</ul>
						</li>
						<li class="nav-item dropdown">
							<a class="nav-link dropdown-toggle navbar-custom main-link" href="#" id="navbarDropdownLife" role="button" data-bs-toggle="dropdown" aria-expanded="false">
								<b class="navbar-custom">La vie du club</b>
							</a>
							<ul class="dropdown-menu" aria-labelledby="navbarDropdownLife">
								<li><a class="dropdown-item" href="{{ path('app_actualites') }}">Actualités</a></li>
								<li><a class="dropdown-item" href="{{ path('app_albums') }}">Galerie photos</a></li>
							</ul>
						</li>
						<a class="nav-link navbar-custom main-link" href="{{ path('app_ressource') }}">
							<b class="navbar-custom">Ressources</b>
						</a>
						<a class="nav-link navbar-custom main-link" href="{{ path('app_contact') }}">
							<b class="navbar-custom">Nous contacter</b>
						</a>
						<li class="nav-item dropdown ms-auto">
							<a class="nav-link dropdown-toggle navbar-custom main-link" href="#" id="navbarDropdownAccount" role="button" data-bs-toggle="dropdown" aria-expanded="false">
								<b class="navbar-custom">Mon compte</b>
							</a>
							<ul class="dropdown-menu" aria-labelledby="navbarDropdownAccount">
								{% if is_granted('IS_AUTHENTICATED_FULLY') %}
									<li><a class="dropdown-item" href="{{ path('app_utilisateur') }}">Mon compte</a></li>
									<li><a class="dropdown-item" href="{{ path('app_utilisateur_changer_mdp') }}">Modifier mon mot de passe</a></li>
									{% if is_granted('ROLE_ADMIN') %}
										<li><a class="dropdown-item" href="{{ path('admin') }}">Accéder à la section admin</a></li>
									{% endif %}
									<li><a class="dropdown-item" href="{{ path('app_logout') }}">Déconnexion</a></li>
								{% else %}
									<li><a class="dropdown-item" href="{{ path('app_inscription') }}">S'inscrire</a></li>
									<li><a class="dropdown-item" href="{{ path('app_login') }}">Se connecter</a></li>
								{% endif %}
							</ul>
						</li>
					</ul>
				</div>
			</div>
		
		</div>
	</div>
</nav>

